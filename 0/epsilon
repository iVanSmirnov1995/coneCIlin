FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 33.148;

boundaryField
{

    inlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }


    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    front
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    symmetry
    {
    	type            symmetryPlane; 
    } 
    
    topAndBottom
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    coneCylinder
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    nozzle
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

}


// ************************************************************************* //
