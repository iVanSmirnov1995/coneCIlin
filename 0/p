FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    front
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    symmetry
    {
    	type            symmetryPlane; 
    }

    topAndBottom
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }

    coneCylinder
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }


    nozzle
    {
        type            fixedValue;
        value           uniform 801325;
    }

}

// ************************************************************************* //
