FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{


    inlet
    {
        type            totalTemperature;
        value           uniform 300;
        T0              uniform 300;
        U               U;
        phi             phi;
        rho             none;
        psi             thermo:psi;
        gamma           1.4;
    }



    outlet
    {
         type            zeroGradient;
    }


    front
    {
        type            freestream;
        freestreamValue           $internalField;
    }

    symmetry
    {
    	type            symmetryPlane; 
    }


    topAndBottom
    {
        type            freestream;
        freestreamValue           $internalField;
    }

    coneCylinder
    {
         type            zeroGradient;
    }


   /* nozzle
    {
    type            fixedValue;
    value uniform 400;
    }*/


}

